{
    "name": "hdruk/laravel-claims-based-access-control",
    "description": "Scoped Claims-Based Access Control for Laravel",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "ClaimsAccessControl\\": "src/"
        }
    },
    "require": {
        "php": "^8.2",
        "illuminate/support": "^12.0"
    },
    "extra": {
        "laravel": {
            "providers": [
            "ClaimsAccessControl\\Providers\\ClaimsAccessControlServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/HDRUK/laravel-claims-based-access-control"
        }
    ]
}
